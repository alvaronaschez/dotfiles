! $OpenBSD: dot.Xdefaults,v 1.3 2014/07/10 10:22:59 jasper Exp $
XTerm*loginShell:true

xterm*foreground: rgb:a8/a8/a8
xterm*background: rgb:00/00/00
xterm*scrollBar: false
xterm*faceName: Monospace
xterm*faceSize: 7

!urxvt*termName: rxvt-256color
urxvt*foreground: rgb:a8/a8/a8
urxvt*background: rgb:00/00/00
urxvt*font: xft:DejaVu Sans Mono:pixelsize=16:antialias=true:hinting=true
urxvt*scrollBar: false
urxvt*geometry: 400x400

