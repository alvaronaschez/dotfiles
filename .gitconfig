[user]
	name = Alvaro Sanchez
	email = alvarosanchezpalomino@gmail.com
[init]
	defaultBranch = master

# delta
# A syntax-highlighting pager for git, diff, grep, and blame output 
# https://github.com/dandavison/delta
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
[merge]
	conflictStyle = zdiff3
